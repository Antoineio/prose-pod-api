{
	"openapi": "3.0.3",
	"info": {
		"title": "prose-pod-api",
		"description": "Prose Pod API server.",
		"contact": {
			"name": "Valerian Saliou",
			"email": "valerian@valeriansaliou.name"
		},
		"license": {
			"name": "MPL-2.0"
		},
		"version": "0.3.1"
	},
	"paths": {
		"/v1/init/first-account": {
			"put": { "$ref": "features/init.json#/paths/init_first_account" }
		},
		"/v1/login": {
			"post": { "$ref": "features/auth.json#/paths/login" }
		},
		"/v1/server/config": {
			"get": { "$ref": "features/server-config.json#/paths/get_server_config" },
			"put": { "$ref": "features/server-config.json#/paths/init_server_config" }
		},
		"/v1/server/config/file-upload-allowed": {
			"put": { "$ref": "features/server-config.json#/paths/set_file_upload_allowed" }
		},
		"/v1/server/config/file-storage-retention": {
			"put": { "$ref": "features/server-config.json#/paths/set_file_storage_retention" }
		},
		"/v1/server/config/file-storage-encryption-scheme": {
			"put": { "$ref": "features/server-config.json#/paths/set_file_storage_encryption_scheme" }
		},
		"/v1/server/config/messaging/reset": {
			"put": { "$ref": "features/server-config.json#/paths/reset_messaging_config" }
		},
		"/v1/server/config/message-archive-retention": {
			"put": { "$ref": "features/server-config.json#/paths/set_message_archive_retention" }
		},
		"/v1/server/config/message-archive-retention/reset": {
			"put": { "$ref": "features/server-config.json#/paths/reset_message_archive_retention" }
		},
		"/v1/server/config/files/reset": {
			"put": { "$ref": "features/server-config.json#/paths/reset_files_config" }
		},
		"/v1/server/config/message-archive-enabled": {
			"put": { "$ref": "features/server-config.json#/paths/set_message_archive_enabled" }
		},
		"/v1/server/config/push-notifications/reset": {
			"put": { "$ref": "features/server-config.json#/paths/reset_push_notifications_config" }
		},
		"/v1/server/config/push-notification-with-body/reset": {
			"put": { "$ref": "features/server-config.json#/paths/reset_push_notification_with_body" }
		},
		"/v1/server/config/push-notification-with-body": {
			"put": { "$ref": "features/server-config.json#/paths/set_push_notification_with_body" }
		},
		"/v1/server/config/push-notification-with-sender/reset": {
			"put": { "$ref": "features/server-config.json#/paths/reset_push_notification_with_sender" }
		},
		"/v1/server/config/push-notification-with-sender": {
			"put": { "$ref": "features/server-config.json#/paths/set_push_notification_with_sender" }
		},
		"/v1/pod/config": {
			"get": { "$ref": "features/pod-config.json#/paths/get_pod_config" }
		},
		"/v1/pod/config/address": {
			"get": { "$ref": "features/pod-config.json#/paths/get_pod_address" },
			"put": { "$ref": "features/pod-config.json#/paths/set_pod_address" }
		},
		"/v1/workspace": {
			"get": { "$ref": "features/workspace.json#/paths/get_workspace" },
			"put": { "$ref": "features/workspace.json#/paths/init_workspace" }
		},
		"/v1/workspace/name": {
			"get": { "$ref": "features/workspace.json#/paths/get_workspace_name" },
			"put": { "$ref": "features/workspace.json#/paths/set_workspace_name" }
		},
		"/v1/workspace/accent-color": {
			"get": { "$ref": "features/workspace.json#/paths/get_workspace_accent_color" },
			"put": { "$ref": "features/workspace.json#/paths/set_workspace_accent_color" }
		},
		"/v1/workspace/icon": {
			"get": { "$ref": "features/workspace.json#/paths/get_workspace_icon" },
			"put": { "$ref": "features/workspace.json#/paths/set_workspace_icon" }
		},
		"/v1/workspace/details-card": {
			"get": { "$ref": "features/workspace.json#/paths/get_workspace_details_card" },
			"put": { "$ref": "features/workspace.json#/paths/set_workspace_details_card" }
		},
		"/v1/invitations": {
			"get": { "$ref": "features/invitations.json#/paths/get_invitations" },
			"post": { "$ref": "features/invitations.json#/paths/invite_member" }
		},
		"/v1/invitations/{invitationIdOrToken}": {
			"get": { "$ref": "features/invitations.json#/paths/get_invitation" }
		},
		"/v1/invitations/{invitationId}": {
			"delete": { "$ref": "features/invitations.json#/paths/invitation_cancel" }
		},
		"/v1/invitations/{token}/accept": {
			"put": { "$ref": "features/invitations.json#/paths/invitation_accept" }
		},
		"/v1/invitations/{token}/reject": {
			"put": { "$ref": "features/invitations.json#/paths/invitation_reject" }
		},
		"/v1/invitations/{invitationId}/resend": {
			"post": { "$ref": "features/invitations.json#/paths/invitation_resend" }
		},
		"/v1/members": {
			"get": { "$ref": "features/members.json#/paths/get_members" }
		},
		"/v1/members/{memberId}": {
			"get": { "$ref": "features/members.json#/paths/get_member" }
		},
		"/v1/enrich-members": {
			"get": { "$ref": "features/members.json#/paths/enrich_members" }
		},
		"/v1/members/{memberId}/role": {
			"put": { "$ref": "features/roles.json#/paths/set_member_role" }
		},
		"/v1/network/dns/records": {
			"get": { "$ref": "features/dns-setup.json#/paths/get_dns_records" }
		},
		"/v1/network/checks": {
			"get": { "$ref": "features/network-checks.json#/paths/check_network_configuration" }
		},
		"/v1/network/checks/dns": {
			"get": { "$ref": "features/network-checks-dns.json#/paths/check_dns_records" }
		},
		"/v1/network/checks/ip": {
			"get": { "$ref": "features/network-checks-ip.json#/paths/check_ip" }
		},
		"/v1/network/checks/ports": {
			"get": { "$ref": "features/network-checks-ports.json#/paths/check_ports" }
		}
	},
	"components": {
		"securitySchemes": { "$ref": "shared.json#/components/securitySchemes" }
	}
}
